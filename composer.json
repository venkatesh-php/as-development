{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "barryvdh/laravel-debugbar": "^3.0",
        "consoletvs/charts": "^5.4",
        "creativeorange/gravatar": "~1.0",
        "doctrine/common": "2.7.0",
        "doctrine/dbal": "2.5.13",
        "fideloper/proxy": "~4.0",
        "google/recaptcha": "^1.1",
        "hashids/hashids": "^2.0",
        "intervention/image": "^2.4",
        "jaybizzle/laravel-crawler-detect": "1.*",
        "jeremykenedy/laravel-exception-notifier": "^1.1",
        "jeremykenedy/laravel-https": "^1.1",
        "jeremykenedy/laravel-logger": "^1.0",
        "jeremykenedy/laravel-phpinfo": "^1.0",
        "jeremykenedy/laravel-roles": "^1.1",
        "jeremykenedy/laravel2step": "^1.0",
        "jeremykenedy/uuid": "^3.1",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.5",
        "league/flysystem": "^1.0",
        "nesbot/carbon": "^1.22",
        "pusher/pusher-php-server": "^3.0",
        "rap2hpoutre/laravel-log-viewer": "^0.10.4",
        "scopdrag/laravel-google-chart": "^1.0",
        "snowfire/beautymail": "dev-master",
        "socialiteproviders/37signals": "^3.0",
        "socialiteproviders/generators": "^2.0",
        "socialiteproviders/instagram": "^3.0",
        "socialiteproviders/twitch": "^3.0",
        "socialiteproviders/youtube": "^3.0",
        "spatie/laravel-permission": "^2.2",
        "symfony/css-selector": "^2.8",
        "symfony/translation": "^2.8",
        "vinkla/hashids": "^3.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~2.0",
        "phpunit/phpunit": "~7.0",
        "symfony/css-selector": "^2.8",
        "symfony/dom-crawler": "3.1.*",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database",
            "database/seeds",
            "database/factories"
        ],
        "files": ["app/Http/helpers.php"],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
        "type": "composer",
        "url": "https://packagist.org"
        },
        { "packagist": false }
        ]
}
